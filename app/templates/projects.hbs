<h2 class="pull-left">Projects</h2>
<div class="container">
  <form>
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>id</th>
          <th>user_id</th>
          <th>remixed_from</th>
          <th>version</th>
          <th>title</th>
          <th>featured</th>
        </tr>
      </thead>
      <tbody>
        {{#each model as |project|}}
          {{#link-to 'project' project tagName="tr"}}
            <td>{{project.id}}</td>
            <td>{{project.user_id}}</td>
            <td>{{project.remixed_from}}</td>
            <td>{{project.version}}</td>
            <td>{{project.title}}</td>
            <td>{{ input type="checkbox" checked=project.featured }}</td>
            <td><button class="btn btn-danger">Delete</button></td>
          {{/link-to}}
        {{/each}}
      </tbody>
    </table>
    <button class="btn btn-primary pull-right">Submit</button>
  </form>
</div>
