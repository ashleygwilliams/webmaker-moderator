<h2 class="pull-left">Projects</h2>
<div class="container">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>id</th>
        <th>user_id</th>
        <th>remixed_from</th>
        <th>version</th>
        <th>featured</th>
      </tr>
    </thead>
    <tbody>
      {{#each model as |project|}}
        <tr>
          <td>{{#link-to 'project.edit' project}}{{project.title}}{{/link-to}}</td>
          <td>{{project.user_id}}</td>
          <td>{{project.remixed_from}}</td>
          <td>{{project.version}}</td>
          <td>{{project.featured}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
